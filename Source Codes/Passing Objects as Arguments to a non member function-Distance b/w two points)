import java.util.Scanner;
import java.math.*;

class Point{
	static float dis;
	float x,y;
		
	public static float cal_dist(Point l,Point m){
		dis=(float)(Math.sqrt(Math.pow((m.x-l.x),2)+Math.pow((m.y-l.y),2)));
		return dis;
	}
	
}
public class PointX {
	public static void main(String[] args){
		Scanner read = new Scanner(System.in);
		Point ob1 = new Point();
		Point ob2 = new Point();
		Point z = new Point();
		float d;
		
		ob1.x=read.nextFloat();
		ob1.y=read.nextFloat();
		ob2.x=read.nextFloat();
		ob2.y=read.nextFloat();
		
		
		d=z.cal_dist(ob1,ob2);
		System.out.print(d);
	}

}
